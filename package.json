{
  "author": "Dennis Gerike",
  "dependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "cors": "2.8.5",
    "express": "4.21.2",
    "neo4j-driver": "5.27.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  },
  "description": "REST API for the open car database 'More Cars'",
  "devDependencies": {
    "@cucumber/cucumber": "11.2.0",
    "@faker-js/faker": "9.3.0",
    "@types/jest": "29.5.14",
    "@usebruno/cli": "1.38.3",
    "ajv": "8.17.1",
    "axios": "1.7.9",
    "dotenv": "16.4.7",
    "jest": "29.7.0",
    "jest-html-reporter": "3.10.2",
    "nodemon": "3.1.9",
    "openapi-schema-validator": "12.1.3",
    "ts-jest": "29.2.5"
  },
  "engines": {
    "node": ">=22"
  },
  "license": "MIT",
  "name": "@more-cars/rest-api",
  "repository": "https://github.com/more-cars/rest-api",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "local:app:start": "node --env-file=.env -r ts-node/register src/server.ts",
    "dev:app:start": "nodemon --watch src -e ts local:app:start",
    "docker:build-image": "docker build -t mc-api -f deployment/Dockerfile .",
    "docker:tag-image": "docker image tag mc-api dennisgerike/more-cars-api:0.1.0",
    "docker:tag-image:dev": "docker image tag mc-api localhost/mc-api:dev",
    "docker:push-image": "docker push dennisgerike/more-cars-api:0.1.0",
    "docker:app:start": "docker run --rm localhost/mc-api:dev",
    "minikube:install": "deployment/minikube-install.sh",
    "minikube:start": "deployment/minikube-cluster-start.sh",
    "minikube:open-tunnel": "deployment/minikube-tunnel-start.sh",
    "minikube:stop": "deployment/minikube-cluster-stop.sh",
    "minikube:delete": "deployment/minikube-cluster-delete.sh",
    "minikube:deploy:dev": "deployment/minikube-app-deploy.sh dev",
    "minikube:deploy:prod": "deployment/minikube-app-deploy.sh prod",
    "minikube:undeploy:dev": "deployment/minikube-app-undeploy.sh dev",
    "minikube:undeploy:prod": "deployment/minikube-app-undeploy.sh prod",
    "minikube:import-dev-image": "minikube image load localhost/mc-api:dev -p mc-api",
    "minikube:delete-dev-image": "minikube image rm localhost/mc-api:dev -p mc-api",
    "gke:deploy:prod": "deployment/gke-app-deploy.sh prod",
    "gke:undeploy:prod": "deployment/gke-app-undeploy.sh prod",
    "tests:bruno:open-gui": "bruno",
    "tests:bruno:run-cli": "cd bruno && bru run --env local",
    "tests:unit": "npx jest --projects tests/unit",
    "tests:integration": "npx jest --projects tests/integration",
    "tests:unit+integration": "npx jest",
    "tests:unit+integration:coverage": "npx jest --coverage",
    "tests:cucumber": "npx cucumber-js",
    "tests:validate-api-schema": "npx ts-node tests/api/schema-validation.ts"
  },
  "version": "0.1.0"
}
