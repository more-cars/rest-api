{
  "author": "Dennis Gerike",
  "dependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "cors": "2.8.5",
    "express": "5.2.1",
    "inflection": "3.0.2",
    "neo4j-driver": "5.28.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "description": "REST API for the open car database 'More Cars'",
  "devDependencies": {
    "@types/cli-progress": "3.11.6",
    "@cucumber/cucumber": "12.4.0",
    "@eslint/js": "9.39.2",
    "@faker-js/faker": "10.1.0",
    "@inquirer/prompts": "8.1.0",
    "@types/k6": "1.4.0",
    "@types/supertest": "6.0.3",
    "@usebruno/cli": "2.15.1",
    "@vitest/coverage-v8": "3.2.4",
    "ajv": "8.17.1",
    "axios": "1.13.2",
    "change-case": "5.4.4",
    "cli-progress": "3.12.0",
    "dotenv": "17.2.3",
    "hygen": "6.2.11",
    "inquirer": "13.1.0",
    "inquirer-file-selector": "1.0.1",
    "moment": "2.30.1",
    "openapi-schema-validator": "12.1.3",
    "sloc": "0.3.2",
    "supertest": "7.1.4",
    "typescript-eslint": "8.50.0",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=24"
  },
  "license": "MIT",
  "name": "@more-cars/rest-api",
  "repository": "https://github.com/more-cars/rest-api",
  "scripts": {
    "start": "NODE_ENV=production npx ts-node src/server.ts",
    "local:app:start": "./deployment/local-app-start.sh",
    "local:db:start": "./deployment/local-db-start.sh",
    "local:hostnames:add": "./deployment/add-hostnames.sh",
    "app:deploy": "./deployment/deploy-app.sh",
    "app:undeploy": "./deployment/undeploy-app.sh",
    "db:migrate": "./migration/run-migration.sh",
    "db:restore": "./migration/restore-mc1-database.sh",
    "docker:build-image": "docker build -t mc-api -f deployment/Dockerfile .",
    "docker:tag-image:dev": "docker image tag mc-api localhost/mc-api:dev",
    "docker:tag-image:dockerhub": "docker image tag mc-api docker.io/dennisgerike/more-cars-api:0.26.0",
    "docker:tag-image:github": "docker image tag mc-api ghcr.io/more-cars/rest-api:0.26.0",
    "docker:login:dockerhub": "docker login docker.io",
    "docker:login:github": "docker login ghcr.io",
    "docker:push-image:dockerhub": "docker push docker.io/dennisgerike/more-cars-api:0.26.0",
    "docker:push-image:github": "docker push ghcr.io/more-cars/rest-api:0.26.0",
    "docker:app:start": "docker run --rm localhost/mc-api:dev",
    "minikube:install": "deployment/minikube-install.sh",
    "minikube:start": "deployment/minikube-cluster-start.sh",
    "minikube:open-tunnel": "deployment/minikube-tunnel-start.sh",
    "minikube:stop": "deployment/minikube-cluster-stop.sh",
    "minikube:delete": "deployment/minikube-cluster-delete.sh",
    "minikube:import-dev-image": "minikube image load localhost/mc-api:dev -p morecars",
    "minikube:delete-dev-image": "minikube image rm localhost/mc-api:dev -p morecars",
    "generate-tickets:node:create": "HYGEN_TMPLS=ticket-generator hygen features create-node && node --env-file=.env -r ts-node/register ticket-generator/create-tickets-for-create-node.ts",
    "generate-tickets": "node --env-file=.env -r ts-node/register ticket-generator/create-tickets.ts",
    "generate-code:node:create": "HYGEN_TMPLS=code_generator hygen node create",
    "generate-code": "node --env-file=.env -r ts-node/register code_generator/generate-code-for-feature.ts",
    "tests:validate-api-schema": "npx ts-node tests/api-validation/schema-validation.ts",
    "tests:lint": "npx eslint src",
    "tests:unit": "npx vitest run -c tests/unit/vite.config.ts",
    "tests:unit:coverage": "npx vitest run -c tests/unit/vite.config.ts --coverage",
    "tests:integration": "./tests/integration/run-tests.sh",
    "tests:developer": "npx vitest run -c tests/vite.config.ts",
    "tests:developer:coverage": "npx vitest run -c tests/vite.config.ts --coverage",
    "tests:smoke:gui": "bruno",
    "tests:smoke:cli": "./tests/smoke/run-tests.sh",
    "tests:behavior": "node --env-file=.env -r ts-node/register tests/behavior/check-dependencies.ts true && npx cucumber-js",
    "tests:behavior:single": "node --env-file=.env -r ts-node/register tests/behavior/run-single-test.ts",
    "tests:performance": "./tests/performance/run-tests.sh",
    "tests:sync-scripts": "vitest run -c tests/sync-scripts/vite.config.ts",
    "stats:lines-of-code:src": "sloc src/",
    "stats:lines-of-code:test": "sloc tests/"
  },
  "version": "0.26.0"
}
