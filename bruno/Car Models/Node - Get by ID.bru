meta {
  name: Get Car Model by ID
  type: http
}

get {
  url: {{baseUrl}}/car-models/{{validCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.data.id: gte 12000000
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  await lib.CarModels.ensureValidCarModelExists()
}
