meta {
  name: Get specific ›has-prime-image‹ relation (valid data)
  type: http
}

get {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/has-prime-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
}

script:pre-request {
  await lib.CarModels.ensureCarModelHasPrimeImageRelationshipExists()
}
