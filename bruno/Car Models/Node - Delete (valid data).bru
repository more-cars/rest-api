meta {
  name: Delete Car Model (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-models/{{validCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.CarModels.ensureValidCarModelExists()
}

script:post-response {
  bru.setEnvVar("validCarModelId", null)
}
