meta {
  name: Delete ›is-successor-of‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/is-successor-of/{{validSecondCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModels.ensureValidSecondCarModelExists()
  await lib.CarModels.ensureCarModelIsSuccessorOfRelationshipExists(bru.getEnvVar('validCarModelId'), bru.getEnvVar('validSecondCarModelId'))
}
