meta {
  name: Create ›has-image‹ relation (valid data)
  type: http
}

post {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/has-image/{{validSecondImageId}}
  body: none
  auth: none
}

assert {
  res.status: eq 201
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: eq has-image
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: image
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.CarModels.ensureValidCarModelExists()
  await lib.Images.ensureValidSecondImageExists()
}
