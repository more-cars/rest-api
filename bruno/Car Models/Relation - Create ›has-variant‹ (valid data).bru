meta {
  name: Create ›has-variant‹ relation (valid data)
  type: http
  tags: [
    create
    relation
    valid
  ]
}

post {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/has-variant/{{validCarModelVariantId}}
  body: none
  auth: none
}

assert {
  res.status: 201
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: has-variant
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: car-model-variant
  res.body.data.relationship_partner.data.id: {{validCarModelVariantId}}
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
}
