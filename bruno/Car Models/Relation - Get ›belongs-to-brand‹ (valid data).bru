meta {
  name: Get ›belongs-to-brand‹ relation (valid data)
  type: http
  tags: [
    get
    relation
    valid
  ]
}

get {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/belongs-to-brand
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: belongs-to-brand
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: brand
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModels.ensureValidCarModelExists()
  await lib.Brands.ensureValidBrandExists()
  await lib.CarModels.ensureCarModelBelongsToBrandRelationshipExists(bru.getEnvVar('validCarModelId'), bru.getEnvVar('validBrandId'))
}
