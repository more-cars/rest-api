meta {
  name: Get all Car Models
  type: http
}

get {
  url: {{baseUrl}}/car-models
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.id: gte 12000000
}

script:pre-request {
  await lib.CarModels.ensureValidCarModelExists()
}
