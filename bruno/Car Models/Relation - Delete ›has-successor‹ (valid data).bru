meta {
  name: Delete ›has-successor‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/has-successor/{{validSecondCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModels.ensureValidSecondCarModelExists()
  await lib.CarModels.ensureCarModelHasSuccessorRelationshipExists(bru.getEnvVar('validCarModelId'), bru.getEnvVar('validSecondCarModelId'))
}
