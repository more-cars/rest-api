meta {
  name: Create ›is-successor-of‹ relation (valid data)
  type: http
}

post {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/is-successor-of/{{validSecondCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 201
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: is-successor-of
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: car-model
  res.body.data.relationship_partner.data.id: {{validSecondCarModelId}}
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModels.ensureValidSecondCarModelExists()
}
