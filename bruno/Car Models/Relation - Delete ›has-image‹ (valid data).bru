meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-models/{{validCarModelId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModels.ensureValidCarModelExists()
  await lib.Images.ensureValidImageExists()
  await lib.CarModels.ensureCarModelHasImageRelationshipExists(bru.getEnvVar('validCarModelId'), bru.getEnvVar('validImageId'))
}
