meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/session-results/{{validSessionResultId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.SessionResults.ensureValidSessionResultExists()
  await lib.Images.ensureValidImageExists()
  await lib.SessionResults.ensureSessionResultHasImageRelationshipExists(bru.getEnvVar('validSessionResultId'), bru.getEnvVar('validImageId'))
}
