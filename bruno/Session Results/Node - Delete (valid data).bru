meta {
  name: Delete Session Result (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/session-results/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.SessionResults.ensureValidSessionResultExists()
}

script:post-response {
  bru.setEnvVar("validSessionResultId", null)
}
