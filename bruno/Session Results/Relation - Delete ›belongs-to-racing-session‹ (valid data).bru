meta {
  name: Delete ›belongs-to-racing-session‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/session-results/{{validSessionResultId}}/belongs-to-racing-session/{{validRacingSessionId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.SessionResults.ensureValidSessionResultExists()
  await lib.RacingSessions.ensureValidRacingSessionExists()
  await lib.SessionResults.ensureSessionResultBelongsToRacingSessionRelationshipExists(bru.getEnvVar('validSessionResultId'), bru.getEnvVar('validRacingSessionId'))
}
