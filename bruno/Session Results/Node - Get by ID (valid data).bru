meta {
  name: Get Session Result by ID (valid data)
  type: http
  tags: [
    get
    node
    valid
  ]
}

get {
  url: {{baseUrl}}/session-results/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.id: {{validSessionResultId}}
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.SessionResults.ensureValidSessionResultExists()
}
