meta {
  name: Get Image by ID
  type: http
}

get {
  url: {{baseUrl}}/images/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.data.id: gte 12000000
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  await lib.Images.ensureValidImageExists()
}
