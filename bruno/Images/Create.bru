meta {
  name: Create
  type: http
  seq: 10
}

post {
  url: {{baseUrl}}/images
  body: json
  auth: none
}

body:json {
  {
    "image_provider": "picci",
    "external_id": "123456"
  }
}

assert {
  res.status: eq 201
  res.body: isJson
  res.body.id: gte 0
}

tests {
  test("both timestamps should be identical", function () {
    const data = res.getBody()
    expect(data.created_at).to.eql(data.updated_at)
  })
}
