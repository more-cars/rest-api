meta {
  name: Delete Image (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/images/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.Images.ensureValidImageExists()
}

script:post-response {
  bru.setEnvVar("validImageId", null)
}
