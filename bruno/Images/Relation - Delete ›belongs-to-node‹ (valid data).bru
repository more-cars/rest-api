meta {
  name: Delete ›belongs-to-node‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/images/{{validImageId}}/belongs-to-node/{{validCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Images.ensureValidImageExists()
  await lib.Brands.ensureValidBrandExists()
  await lib.Images.ensureImageBelongsToNodeRelationshipExists(bru.getEnvVar('validImageId'), bru.getEnvVar('validBrandId'))
}
