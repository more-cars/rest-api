meta {
  name: Delete Image
  type: http
}

delete {
  url: {{baseUrl}}/images/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: eq 204
  res.body: isEmpty
}

script:pre-request {
  await lib.Images.ensureValidImageExists()
}

script:post-response {
  bru.setEnvVar("validImageId", null)
}
