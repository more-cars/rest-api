meta {
  name: Delete ›belongs-to-node‹ relation (invalid data)
  type: http
  tags: [
    delete
    relation
    invalid
  ]
}

delete {
  url: {{baseUrl}}/images/{{invalidImageId}}/belongs-to-node/{{invalidNodeId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
