meta {
  name: Get all ›belongs-to-node‹ relations (valid data)
  type: http
}

get {
  url: {{baseUrl}}/images/{{validImageId}}/belongs-to-node
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.relationship_id: gte 12000000
  res.body.data[0].data.relationship_name: belongs-to-node
  res.body.data[0].data.relationship_partner: isJson
  res.body.data[0].data.relationship_partner.node_type: isString
  res.body.data[0].data.relationship_partner.data.id: gte 12000000
  res.body.data[0].data.created_at: isString
  res.body.data[0].data.updated_at: isString
}

script:pre-request {
  await lib.Images.ensureImageBelongsToNodeRelationshipExists()
}
