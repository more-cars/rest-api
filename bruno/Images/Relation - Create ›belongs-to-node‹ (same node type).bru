meta {
  name: Create ›belongs-to-node‹ relation (same node type)
  type: http
  tags: [
    create
    relation
    invalid
  ]
}

post {
  url: {{baseUrl}}/images/{{validImageId}}/belongs-to-node/{{validSecondImageId}}
  body: none
  auth: none
}

assert {
  res.status: 422
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Images.ensureValidImageExists()
  await lib.Images.ensureValidSecondImageExists()
}
