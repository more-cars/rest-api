meta {
  name: Create Image (valid data)
  type: http
  tags: [
    create
    node
    valid
  ]
}

post {
  url: {{baseUrl}}/images
  body: json
  auth: none
}

body:json {
  {
    "image_provider": "picci",
    "external_id": "123456"
  }
}

assert {
  res.status: 201
  res.body: isJson
  res.body.data.id: gte 12000000
}

tests {
  test("both timestamps should be identical", function () {
    const data = res.getBody()
    expect(data.data.created_at).to.eql(data.data.updated_at)
  })
}
