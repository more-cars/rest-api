meta {
  name: Delete ›belongs-to-race-track‹ relation (invalid data)
  type: http
  tags: [
    delete
    relation
    invalid
  ]
}

delete {
  url: {{baseUrl}}/track-layouts/{{invalidTrackLayoutId}}/belongs-to-race-track/{{invalidRaceTrackId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  lib.tools.generateInvalidId('TrackLayout')
  lib.tools.generateInvalidId('RaceTrack')
}
