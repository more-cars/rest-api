meta {
  name: Delete Track Layout (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/track-layouts/{{validTrackLayoutId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.TrackLayouts.ensureValidTrackLayoutExists()
}

script:post-response {
  bru.setEnvVar("validTrackLayoutId", null)
}
