meta {
  name: Delete ›was-used-by-racing-event‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/track-layouts/{{validTrackLayoutId}}/was-used-by-racing-event/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.TrackLayouts.ensureValidTrackLayoutExists()
  await lib.RacingEvents.ensureValidRacingEventExists()
  await lib.TrackLayouts.ensureTrackLayoutWasUsedByRacingEventRelationshipExists(bru.getEnvVar('validTrackLayoutId'), bru.getEnvVar('validRacingEventId'))
}
