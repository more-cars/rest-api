meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/track-layouts/{{validTrackLayoutId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.TrackLayouts.ensureValidTrackLayoutExists()
  await lib.Images.ensureValidImageExists()
  await lib.TrackLayouts.ensureTrackLayoutHasImageRelationshipExists(bru.getEnvVar('validTrackLayoutId'), bru.getEnvVar('validImageId'))
}
