meta {
  name: Get Racing Event by ID (valid data)
  type: http
}

get {
  url: {{baseUrl}}/racing-events/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.id: {{validRacingEventId}}
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  await lib.RacingEvents.ensureValidRacingEventExists()
}
