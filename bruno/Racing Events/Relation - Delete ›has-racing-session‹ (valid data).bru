meta {
  name: Delete ›has-racing-session‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-events/{{validRacingEventId}}/has-racing-session/{{validRacingSessionId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingEvents.ensureValidRacingEventExists()
  await lib.RacingSessions.ensureValidRacingSessionExists()
  await lib.RacingEvents.ensureRacingEventHasRacingSessionRelationshipExists(bru.getEnvVar('validRacingEventId'), bru.getEnvVar('validRacingSessionId'))
}
