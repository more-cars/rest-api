meta {
  name: Get all Racing Events (valid sort)
  type: http
}

get {
  url: {{baseUrl}}/racing-events?sort_by_property=name&sort_direction=desc
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.id: gte 12000000
}
