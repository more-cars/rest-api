meta {
  name: Get all ›has-racing-session‹ relations (valid data)
  type: http
}

get {
  url: {{baseUrl}}/racing-events/{{validRacingEventId}}/has-racing-session
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.relationship_id: gte 12000000
  res.body.data[0].data.relationship_name: has-racing-session
  res.body.data[0].data.relationship_partner: isJson
  res.body.data[0].data.relationship_partner.node_type: racing-session
  res.body.data[0].data.relationship_partner.data.id: gte 12000000
  res.body.data[0].data.created_at: isString
  res.body.data[0].data.updated_at: isString
}

script:pre-request {
  await lib.RacingEvents.ensureRacingEventHasRacingSessionRelationshipExists()
}
