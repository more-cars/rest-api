meta {
  name: Get all Racing Events
  type: http
  tags: [
    get
    node
    collection
    valid
  ]
}

get {
  url: {{baseUrl}}/racing-events
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.id: gte 12000000
}

script:pre-request {
  await lib.RacingEvents.ensureValidRacingEventExists()
}
