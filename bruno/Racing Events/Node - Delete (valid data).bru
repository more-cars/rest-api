meta {
  name: Delete Racing Event (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-events/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.RacingEvents.ensureValidRacingEventExists()
}

script:post-response {
  bru.setEnvVar("validRacingEventId", null)
}
