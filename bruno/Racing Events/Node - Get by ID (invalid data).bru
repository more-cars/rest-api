meta {
  name: Get Racing Event by ID (invalid data)
  type: http
  tags: [
    get
    node
    invalid
  ]
}

get {
  url: {{baseUrl}}/racing-events/{{invalidRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  lib.tools.generateInvalidId('RacingEvent')
}
