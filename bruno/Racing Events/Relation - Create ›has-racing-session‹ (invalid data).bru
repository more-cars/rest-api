meta {
  name: Create ›has-racing-session‹ relation (invalid data)
  type: http
  tags: [
    create
    relation
    invalid
  ]
}

post {
  url: {{baseUrl}}/racing-events/{{invalidRacingEventId}}/has-racing-session/{{invalidRacingSessionId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  lib.tools.generateInvalidId('RacingEvent')
  lib.tools.generateInvalidId('RacingSession')
}
