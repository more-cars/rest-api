meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-events/{{validRacingEventId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingEvents.ensureValidRacingEventExists()
  await lib.Images.ensureValidImageExists()
  await lib.RacingEvents.ensureRacingEventHasImageRelationshipExists(bru.getEnvVar('validRacingEventId'), bru.getEnvVar('validImageId'))
}
