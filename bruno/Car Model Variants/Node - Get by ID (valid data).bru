meta {
  name: Get Car Model Variant by ID (valid data)
  type: http
  tags: [
    get
    node
    valid
  ]
}

get {
  url: {{baseUrl}}/car-model-variants/{{validCarModelVariantId}}
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.id: {{validCarModelVariantId}}
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
}
