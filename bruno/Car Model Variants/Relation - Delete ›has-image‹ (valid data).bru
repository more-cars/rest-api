meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-model-variants/{{validCarModelVariantId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
  await lib.Images.ensureValidImageExists()
  await lib.CarModelVariants.ensureCarModelVariantHasImageRelationshipExists(bru.getEnvVar('validCarModelVariantId'), bru.getEnvVar('validImageId'))
}
