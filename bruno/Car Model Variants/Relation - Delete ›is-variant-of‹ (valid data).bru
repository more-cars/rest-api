meta {
  name: Delete ›is-variant-of‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-model-variants/{{validCarModelVariantId}}/is-variant-of/{{validCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModelVariants.ensureCarModelVariantIsVariantOfRelationshipExists(bru.getEnvVar('validCarModelVariantId'), bru.getEnvVar('validCarModelId'))
}
