meta {
  name: Delete ›achieved-session-result‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/car-model-variants/{{validCarModelVariantId}}/achieved-session-result/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
  await lib.SessionResults.ensureValidSessionResultExists()
  await lib.CarModelVariants.ensureCarModelVariantAchievedSessionResultRelationshipExists(bru.getEnvVar('validCarModelVariantId'), bru.getEnvVar('validSessionResultId'))
}
