meta {
  name: Get ›is-variant-of‹ relation (valid data)
  type: http
  tags: [
    get
    relation
    valid
  ]
}

get {
  url: {{baseUrl}}/car-model-variants/{{validCarModelVariantId}}/is-variant-of
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: is-variant-of
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: car-model
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
  await lib.CarModels.ensureValidCarModelExists()
  await lib.CarModelVariants.ensureCarModelVariantIsVariantOfRelationshipExists(bru.getEnvVar('validCarModelVariantId'), bru.getEnvVar('validCarModelId'))
}
