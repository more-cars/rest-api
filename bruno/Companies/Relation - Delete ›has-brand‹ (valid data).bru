meta {
  name: Delete ›has-brand‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/companies/{{validCompanyId}}/has-brand/{{validBrandId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Companies.ensureValidCompanyExists()
  await lib.Brands.ensureValidBrandExists()
  await lib.Companies.ensureCompanyHasBrandRelationshipExists(bru.getEnvVar('validCompanyId'), bru.getEnvVar('validBrandId'))
}
