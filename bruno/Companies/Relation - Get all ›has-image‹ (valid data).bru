meta {
  name: Get all ›has-image‹ relations (valid data)
  type: http
}

get {
  url: {{baseUrl}}/companies/{{validCompanyId}}/has-image
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body: isArray
}

script:pre-request {
  await lib.Companies.ensureCompanyHasImageRelationshipExists()
}
