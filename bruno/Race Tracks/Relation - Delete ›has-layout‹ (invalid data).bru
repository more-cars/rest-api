meta {
  name: Delete ›has-layout‹ relation (invalid data)
  type: http
  tags: [
    delete
    relation
    invalid
  ]
}

delete {
  url: {{baseUrl}}/race-tracks/{{invalidRaceTrackId}}/has-layout/{{invalidTrackLayoutId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  lib.tools.generateInvalidId('RaceTrack')
  lib.tools.generateInvalidId('TrackLayout')
}
