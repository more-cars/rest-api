  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/race-tracks/{{validRaceTrackId}}/hosted-racing-event/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
