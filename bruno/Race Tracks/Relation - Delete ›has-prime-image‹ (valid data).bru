meta {
  name: Delete ›has-prime-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/race-tracks/{{validRaceTrackId}}/has-prime-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RaceTracks.ensureValidRaceTrackExists()
  await lib.Images.ensureValidImageExists()
  await lib.RaceTracks.ensureRaceTrackHasPrimeImageRelationshipExists(bru.getEnvVar('validRaceTrackId'), bru.getEnvVar('validImageId'))
}
