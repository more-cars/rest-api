meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-series/{{validRacingSeriesId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSeries.ensureValidRacingSeriesExists()
  await lib.Images.ensureValidImageExists()
  await lib.RacingSeries.ensureRacingSeriesHasImageRelationshipExists(bru.getEnvVar('validRacingSeriesId'), bru.getEnvVar('validImageId'))
}
