meta {
  name: Get ›has-prime-image‹ relation (valid data)
  type: http
  tags: [
    get
    relation
    valid
  ]
}

get {
  url: {{baseUrl}}/racing-series/{{validRacingSeriesId}}/has-prime-image
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: has-prime-image
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: image
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.RacingSeries.ensureRacingSeriesHasPrimeImageRelationshipExists()
}
