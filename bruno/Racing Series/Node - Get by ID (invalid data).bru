meta {
  name: Get Racing Series by ID (invalid data)
  type: http
  tags: [
    get
    node
    invalid
  ]
}

get {
  url: {{baseUrl}}/racing-series/{{invalidRacingSeriesId}}
  body: none
  auth: none
}

assert {
  res.status: 404
  res.body: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  lib.tools.generateInvalidId('RacingSeries')
}
