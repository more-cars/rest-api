meta {
  name: Create ›has-prime-image‹ relation (valid data)
  type: http
}

post {
  url: {{baseUrl}}/racing-series/{{validRacingSeriesId}}/has-prime-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 201
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: has-prime-image
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: image
  res.body.data.relationship_partner.data.id: {{validImageId}}
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.RacingSeries.ensureValidRacingSeriesExists()
  await lib.Images.ensureValidImageExists()
}
