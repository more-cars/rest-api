meta {
  name: Delete ›has-racing-event‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-series/{{validRacingSeriesId}}/has-racing-event/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSeries.ensureValidRacingSeriesExists()
  await lib.RacingEvents.ensureValidRacingEventExists()
  await lib.RacingSeries.ensureRacingSeriesHasRacingEventRelationshipExists(bru.getEnvVar('validRacingSeriesId'), bru.getEnvVar('validRacingEventId'))
}
