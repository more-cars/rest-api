meta {
  name: Delete Racing Series (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-series/{{validRacingSeriesId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSeries.ensureValidRacingSeriesExists()
}

script:post-response {
  bru.setEnvVar("validRacingSeriesId", null)
}
