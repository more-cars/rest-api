meta {
  name: Get ›belongs-to-company‹ relation (valid data)
  type: http
}

get {
  url: {{baseUrl}}/brands/{{validBrandId}}/belongs-to-company
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: belongs-to-company
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: company
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.Brands.ensureBrandBelongsToCompanyRelationshipExists()
}
