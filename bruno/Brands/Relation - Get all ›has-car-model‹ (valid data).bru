meta {
  name: Get all ›has-car-model‹ relations (valid data)
  type: http
  tags: [
    get
    relation
    collection
    valid
  ]
}

get {
  url: {{baseUrl}}/brands/{{validBrandId}}/has-car-model
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.relationship_id: gte 12000000
  res.body.data[0].data.relationship_name: has-car-model
  res.body.data[0].data.relationship_partner: isJson
  res.body.data[0].data.relationship_partner.node_type: car-model
  res.body.data[0].data.relationship_partner.data.id: gte 12000000
  res.body.data[0].data.created_at: isString
  res.body.data[0].data.updated_at: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Brands.ensureValidBrandExists()
  await lib.CarModels.ensureValidCarModelExists()
  await lib.Brands.ensureBrandHasCarModelRelationshipExists(bru.getEnvVar('validBrandId'), bru.getEnvVar('validCarModelId'))
}
