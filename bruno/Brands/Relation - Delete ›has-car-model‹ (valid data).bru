meta {
  name: Delete ›has-car-model‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/brands/{{validBrandId}}/has-car-model/{{validCarModelId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Brands.ensureValidBrandExists()
  await lib.CarModels.ensureValidCarModelExists()
  await lib.Brands.ensureBrandHasCarModelRelationshipExists(bru.getEnvVar('validBrandId'), bru.getEnvVar('validCarModelId'))
}
