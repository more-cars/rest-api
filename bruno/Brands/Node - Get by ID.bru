meta {
  name: Get Brand by ID
  type: http
}

get {
  url: {{baseUrl}}/brands/{{validBrandId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.id: gte 12000000
  res.body.created_at: isNotEmpty
  res.body.updated_at: isNotEmpty
}

script:pre-request {
  await lib.Brands.ensureValidBrandExists()
}
