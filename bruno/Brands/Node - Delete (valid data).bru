meta {
  name: Delete Brand (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/brands/{{validBrandId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.Brands.ensureValidBrandExists()
}

script:post-response {
  bru.setEnvVar("validBrandId", null)
}
