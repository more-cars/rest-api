meta {
  name: Delete Brand
  type: http
}

delete {
  url: {{baseUrl}}/brands/{{validBrandId}}
  body: none
  auth: none
}

assert {
  res.status: eq 204
  res.body: isEmpty
}

script:pre-request {
  await lib.Brands.ensureValidBrandExists()
}

script:post-response {
  bru.setEnvVar("validBrandId", null)
}
