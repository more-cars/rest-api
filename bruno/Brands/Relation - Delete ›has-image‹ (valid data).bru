meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/brands/{{validBrandId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.Brands.ensureValidBrandExists()
  await lib.Images.ensureValidImageExists()
  await lib.Brands.ensureBrandHasImageRelationshipExists(bru.getEnvVar('validBrandId'), bru.getEnvVar('validImageId'))
}
