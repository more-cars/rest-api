meta {
  name: Get ›achieved-with-car-model-variant‹ relation (valid data)
  type: http
  tags: [
    get
    relation
    valid
  ]
}

get {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/achieved-with-car-model-variant
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: achieved-with-car-model-variant
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: car-model-variant
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
  await lib.CarModelVariants.ensureValidCarModelVariantExists()
  await lib.LapTimes.ensureLapTimeAchievedWithCarModelVariantRelationshipExists(bru.getEnvVar('validLapTimeId'), bru.getEnvVar('validCarModelVariantId'))
}
