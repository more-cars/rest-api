meta {
  name: Delete ›belongs-to-session-result‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/belongs-to-session-result/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
  await lib.SessionResults.ensureValidSessionResultExists()
  await lib.LapTimes.ensureLapTimeBelongsToSessionResultRelationshipExists(bru.getEnvVar('validLapTimeId'), bru.getEnvVar('validSessionResultId'))
}
