meta {
  name: Create ›belongs-to-session-result‹ relation (valid data)
  type: http
}

post {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/belongs-to-session-result/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 201
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: belongs-to-session-result
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: session-result
  res.body.data.relationship_partner.data.id: {{validSessionResultId}}
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.LapTimes.ensureValidLapTimeExists()
  await lib.SessionResults.ensureValidSessionResultExists()
}
