meta {
  name: Get all Lap Times
  type: http
  tags: [
    get
    node
    collection
    valid
  ]
}

get {
  url: {{baseUrl}}/lap-times
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body.data: isArray
  res.body.data[0].data: isJson
  res.body.data[0].data.id: gte 12000000
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
}
