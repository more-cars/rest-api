meta {
  name: Delete ›achieved-on-track-layout‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/achieved-on-track-layout/{{validTrackLayoutId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
  await lib.TrackLayouts.ensureValidTrackLayoutExists()
  await lib.LapTimes.ensureLapTimeAchievedOnTrackLayoutRelationshipExists(bru.getEnvVar('validLapTimeId'), bru.getEnvVar('validTrackLayoutId'))
}
