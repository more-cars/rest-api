meta {
  name: Get ›belongs-to-session-result‹ relation (valid data)
  type: http
  tags: [
    get
    relation
    valid
  ]
}

get {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/belongs-to-session-result
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.relationship_id: gte 12000000
  res.body.data.relationship_name: belongs-to-session-result
  res.body.data.relationship_partner: isJson
  res.body.data.relationship_partner.node_type: session-result
  res.body.data.relationship_partner.data.id: gte 12000000
  res.body.data.created_at: isString
  res.body.data.updated_at: isString
}

script:pre-request {
  await lib.LapTimes.ensureLapTimeBelongsToSessionResultRelationshipExists()
}
