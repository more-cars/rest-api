meta {
  name: Delete Lap Time (valid data)
  type: http
}

delete {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.LapTimes.ensureValidLapTimeExists()
}

script:post-response {
  bru.setEnvVar("validLapTimeId", null)
}
