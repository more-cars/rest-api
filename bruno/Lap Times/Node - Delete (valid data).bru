meta {
  name: Delete Lap Time (valid data)
  type: http
  tags: [
    delete
    node
    valid
  ]
}

delete {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
}

script:post-response {
  bru.setEnvVar("validLapTimeId", null)
}
