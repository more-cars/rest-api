meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/lap-times/{{validLapTimeId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.LapTimes.ensureValidLapTimeExists()
  await lib.Images.ensureValidImageExists()
  await lib.LapTimes.ensureLapTimeHasImageRelationshipExists(bru.getEnvVar('validLapTimeId'), bru.getEnvVar('validImageId'))
}
