meta {
  name: Get Racing Session by ID (valid data)
  type: http
  tags: [
    get
    node
    valid
  ]
}

get {
  url: {{baseUrl}}/racing-sessions/{{validRacingSessionId}}
  body: none
  auth: none
}

assert {
  res.status: 200
  res.body: isJson
  res.body.data.id: {{validRacingSessionId}}
  res.body.data.created_at: isNotEmpty
  res.body.data.updated_at: isNotEmpty
}

script:pre-request {
  await lib.RacingSessions.ensureValidRacingSessionExists()
}
