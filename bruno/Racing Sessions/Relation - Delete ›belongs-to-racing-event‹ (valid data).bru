meta {
  name: Delete ›belongs-to-racing-event‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-sessions/{{validRacingSessionId}}/belongs-to-racing-event/{{validRacingEventId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSessions.ensureValidRacingSessionExists()
  await lib.RacingEvents.ensureValidRacingEventExists()
  await lib.RacingSessions.ensureRacingSessionBelongsToRacingEventRelationshipExists(bru.getEnvVar('validRacingSessionId'), bru.getEnvVar('validRacingEventId'))
}
