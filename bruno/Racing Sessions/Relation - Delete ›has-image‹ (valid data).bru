meta {
  name: Delete ›has-image‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-sessions/{{validRacingSessionId}}/has-image/{{validImageId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSessions.ensureValidRacingSessionExists()
  await lib.Images.ensureValidImageExists()
  await lib.RacingSessions.ensureRacingSessionHasImageRelationshipExists(bru.getEnvVar('validRacingSessionId'), bru.getEnvVar('validImageId'))
}
