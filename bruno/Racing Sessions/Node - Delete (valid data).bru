meta {
  name: Delete Racing Session (valid data)
  type: http
}

delete {
  url: {{baseUrl}}/racing-sessions/{{validRacingSessionId}}
  body: none
  auth: none
}

assert {
  res.status: 204
  res.body: isEmpty
}

script:pre-request {
  await lib.RacingSessions.ensureValidRacingSessionExists()
}

script:post-response {
  bru.setEnvVar("validRacingSessionId", null)
}
