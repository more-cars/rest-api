meta {
  name: Delete ›has-session-result‹ relation (valid data)
  type: http
  tags: [
    delete
    relation
    valid
  ]
}

delete {
  url: {{baseUrl}}/racing-sessions/{{validRacingSessionId}}/has-session-result/{{validSessionResultId}}
  body: none
  auth: none
}

assert {
  res.status: 204
}

script:pre-request {
  const lib = require ('./lib/lib')
  await lib.RacingSessions.ensureValidRacingSessionExists()
  await lib.SessionResults.ensureValidSessionResultExists()
  await lib.RacingSessions.ensureRacingSessionHasSessionResultRelationshipExists(bru.getEnvVar('validRacingSessionId'), bru.getEnvVar('validSessionResultId'))
}
